    rel:belongs <Stark> .
        
<#Bran_Stark>
    <Status> "dead";
    rel:kidOf <#Ned_Stark>;
    rel:kidOf <#Cathelyn_Stark>;
    a foaf:Person;
    foaf:name "Bran Stark";
    rel:belongs <Stark>.

<#Jon_Snow>
    <Status> "alive";
    rel:stepSonOf <#Ned_Stark>;
    rel:stepSon <#Cathelyn_Stark>;
    rel:nephewOf <#Ned_Stark>;
    rel:kills <Daenerys_Targaryen>;
    rel:inLoveWith <Daenerys_Targaryen>;
    rel:nephewOf <Daenerys_Targaryen>;
    a foaf:Person;
    foaf:name "Jon Snow";
    rel:belongs <Stark>.
    
<#Night_King>
    <Status> "dead";
    rel:enemyOf <#Bran_Stark>;
    rel:killedBy <#Arya_Stark>;
