    foaf:name "Arya Stark";
    rel:belongs <Stark>.
    
<#Sansa_Stark>
    <Status> "alive";
    rel:exSpouseOf <#Tyrion_Lannister>;
    rel:exFianceeOf <#Joffrey_Baratheon>;
    rel:kidOf <#Ned_Stark>;
    rel:kidOf <#Cathelyn_Stark>;
    a foaf:Person;
    foaf:name "Sansa Stark";
