@base <http://uri/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:       <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rel: <http://www.perceive.net/schemas/relationship/> .
@prefix type: <http://www.perceive.net/schemas/type/> .


rel:belongs
    a           rdfs:Property ;
    rdfs:domain type:Person ;
    rdfs:range  type:House .

<#Cathelyn_Stark>
    <Status> "dead";
    rel:marriedTo <#Ned_Stark> ;
    rel:siblingOf <#Lysa_Aryn> ;
    a foaf:Person ;
    foaf:name "Cathelyn Stark";
    rel:belongs <Stark> .

<#Ned_Stark>
    <Status> "dead";
    rel:marriedTo <#Cathelyn_Stark> ;
    a foaf:Person ; #a - jest typu
    foaf:name "Ned Stark";
    rel:belongs <Stark> .

<#Benjen_Stark>
    <Status> "dead";
    rel:friendOf <#Jon_Snow>;
    rel:siblingOf <#Ned_Stark>;
    a foaf:Person ;
    foaf:name "Benjen Stark";
    rel:belongs <Stark> .

<#Jeor_Mormont>
