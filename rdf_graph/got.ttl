<#Viserys_Targaryen>
    <Status> "dead";
    a foaf:Person;
    foaf:name "Viserys Targaryen";
    rel:belongs <Targaryen>.
    
<#Joffrey_Baratheon>
    <Status> "dead";
    rel:marriedTo <#Margery_Tyrell>;
    rel:kidOf <#Robert_Baratheon>;
    rel:exFianceeOf <#Sansa_Stark>;
    a foaf:Person ;
    foaf:name "Joffrey Baratheon";
    rel:belongs <Baratheon> .

