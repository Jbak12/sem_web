    rel:commanderOf <#Samwell_Tarly>;
    rel:commanderOf <#Benjen_Stark>;
    a foaf:Person ;
    foaf:name "Jeor Mormont";
    rel:belongs <Other> .

<#Petyr_LittleFinger>
