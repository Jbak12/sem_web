    rel:friendOf <#Ned_Stark>;
    a foaf:Person ;
    foaf:name "Robert Baratheon";
    rel:belongs <Baratheon>.

<#Gendry>
    <Status> "dead";
    rel:friendOf <#Arya_Stark>;
    rel:kidOf <#Robert_Baratheon>;
    a foaf:Person;
    foaf:name "Gendry" ;
    rel:belongs <Other> .

<#Cersei_Lannister>
